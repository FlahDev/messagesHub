{
	"name": "workspace",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"license": "ISC",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:staged": "yarn --silent lint-staged --allow-empty",
		"lint:clear-cache": "rm ./.eslintcache",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"cli:update-hooks": "yarn lefthook install",
		"cli:add-hook": "yarn lefthook add -d",
		"cli:test-hook": "yarn lefthook run 2>/dev/null",
		"cli:reset-all-hooks": "git config --unset core.hooksPath || rm -rf .git/hooks/*",
		"cli:init-hooks": "yarn cli:reset-all-hooks && yarn install && yarn cli:update-hooks"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.4",
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@inquirer/confirm": "^5.1.6",
		"@inquirer/prompts": "^7.3.2",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-essentials": "^8.5.6",
		"@storybook/addon-interactions": "^8.5.6",
		"@storybook/addon-svelte-csf": "^5.0.0-next.23",
		"@storybook/blocks": "^8.5.6",
		"@storybook/svelte": "^8.5.6",
		"@storybook/sveltekit": "^8.5.6",
		"@storybook/test": "^8.5.6",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"chalk": "^5.4.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^25.0.1",
		"lefthook": "^1.10.10",
		"lint-staged": "^15.4.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"storybook": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"axios": "^1.7.9",
		"styled-svelte5": "^0.4.1",
		"svelte-routing": "^2.13.0"
	}
}
